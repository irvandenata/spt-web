<table class="table" id="table1">
	<thead>
		<tr>
			<th width="5%">No.</th>
			<th>Lokasi Berangkat</th>
			<th>Lokasi Tujuan</th>
			<th>Alokasi Dana</th>
			<th width="15%">Aksi</th>
		</tr>
	</thead>
	<tbody>
		<% for(let i = 0 ; i < tickets.length; i++ ) {%>
		<tr>
			<td><%= i+1 %></td>
			<td><%= tickets[i].from?.name??"-" %></td>
			<td><%= tickets[i].destination?.name??"-" %></td>
			<td>
				Rp. <%= parseInt(tickets[i].nominal).toFixed(2)
				.replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,"); %>
			</td>
			<td>
				<a
					href="/<%= current%>/<%= tickets[i]._id %>/edit"
					class="btn btn-warning btn-sm"
					>Edit</a
				>
				<form
					action="/<%= current%>/delete/<%= tickets[i]._id %>?_method=DELETE"
					method="POST"
				></form>
				<button class="btn btn-sm btn-danger mt-1 ml-1 btn-delete">
					Hapus
				</button>
			</td>
		</tr>
		<%}%>
	</tbody>
</table>
