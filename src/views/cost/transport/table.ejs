<table class="table" id="table1">
	<thead>
		<tr>
			<th width="5%">No.</th>
			<th>Nama Lokasi</th>
			<th>Alokasi Dana</th>
			<th width="15%">Aksi</th>
		</tr>
	</thead>
	<tbody>
		<% for(let i = 0 ; i < transports.length; i++ ) {%>
		<tr>
			<td><%= i+1 %></td>
			<td><%= transports[i].district?.name??"-" %></td>
			<td>
				Rp. <%= parseInt(transports[i].nominal).toFixed(2)
				.replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,"); %>
			</td>
			<td>
				<a
					href="/<%= current%>/<%= transports[i]._id %>/edit"
					class="btn btn-warning btn-sm"
					>Edit</a
				>
				<form
					action="/<%= current%>/delete/<%= transports[i]._id %>?_method=DELETE"
					method="POST"
				></form>
				<button class="btn btn-sm btn-danger mt-1 ml-1 btn-delete">
					Hapus
				</button>
			</td>
		</tr>
		<%}%>
	</tbody>
</table>
